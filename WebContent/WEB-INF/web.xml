<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>SpecUp</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>

	<!-- USER -->
	<servlet>
		<servlet-name>UserRegisterServlet</servlet-name>
		<servlet-class>user.UserRegisterServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UserRegisterServlet</servlet-name>
		<url-pattern>/userRegister</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>UserLoginServlet</servlet-name>
		<servlet-class>user.UserLoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UserLoginServlet</servlet-name>
		<url-pattern>/userLogin</url-pattern>
	</servlet-mapping>

	<!-- PROJECT -->
	<servlet>
		<servlet-name>ProjectInsertServlet</servlet-name>
		<servlet-class>project.ProjectInsertServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ProjectInsertServlet</servlet-name>
		<url-pattern>/projectInsert</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ProjectDeleteServlet</servlet-name>
		<servlet-class>project.ProjectDeleteServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ProjectDeleteServlet</servlet-name>
		<url-pattern>/projectDelete</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ProjectUpdateServlet</servlet-name>
		<servlet-class>project.ProjectUpdateServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ProjectUpdateServlet</servlet-name>
		<url-pattern>/projectUpdate</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ProjectDetailServlet</servlet-name>
		<servlet-class>project.ProjectDetailServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ProjectDetailServlet</servlet-name>
		<url-pattern>/ProjectDetail</url-pattern>
	</servlet-mapping>
	
	<!-- ACTIVE -->
	<servlet>
		<servlet-name>ActiveInsertServlet</servlet-name>
		<servlet-class>active.ActiveInsertServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ActiveInsertServlet</servlet-name>
		<url-pattern>/activeInsert</url-pattern>
	</servlet-mapping>
	
	 
	<servlet>
		<servlet-name>ActiveDeleteServlet</servlet-name>
		<servlet-class>active.ActiveDeleteServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ActiveDeleteServlet</servlet-name>
		<url-pattern>/activeDelete</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ActiveUpdateServlet</servlet-name>
		<servlet-class>active.ActiveUpdateServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ActiveUpdateServlet</servlet-name>
		<url-pattern>/activeUpdate</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ActiveDetailServlet</servlet-name>
		<servlet-class>active.ActiveDetailServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ActiveDetailServlet</servlet-name>
		<url-pattern>/ActiveDetail</url-pattern>
	</servlet-mapping>
</web-app>